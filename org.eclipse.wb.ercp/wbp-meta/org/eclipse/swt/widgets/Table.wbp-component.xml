<?xml version="1.0" encoding="UTF-8"?>
<component xmlns="http://www.eclipse.org/wb/WBPComponent">
	<model class="org.eclipse.wb.internal.swt.model.widgets.TableInfo"/>
	<description>Instances of this class implement a selectable user interface object that displays a list of
		images and strings and issue notificiation when selected.</description>
	<!-- CREATION -->
	<creation>
		<source><![CDATA[new org.eclipse.swt.widgets.Table(%parent%, org.eclipse.swt.SWT.BORDER | org.eclipse.swt.SWT.FULL_SELECTION)]]></source>
		<invocation signature="setLinesVisible(boolean)"><![CDATA[true]]></invocation>
		<invocation signature="setHeaderVisible(boolean)"><![CDATA[true]]></invocation>
	</creation>
	<!-- CONSTRUCTORS -->
	<constructors>
		<constructor>
			<parameter type="org.eclipse.swt.widgets.Composite" parent="true"/>
			<parameter type="int" defaultSource="org.eclipse.swt.SWT.NONE">
				<editor id="style">
					<parameter name="class">org.eclipse.swt.SWT</parameter>
					<parameter name="set">BORDER CHECK FULL_SELECTION HIDE_SELECTION</parameter>
					<parameter name="select0">selection SINGLE SINGLE MULTI</parameter>
				</editor>
			</parameter>
		</constructor>
	</constructors>
	<!-- METHODS -->
	<methods>
		<!-- SELECTION -->
		<method name="select">
			<parameter type="int"/>
			<parameter type="int"/>
		</method>
		<method name="select">
			<parameter type="int[]"/>
		</method>
		<method name="setSelection">
			<parameter type="int"/>
			<parameter type="int"/>
		</method>
		<method name="setSelection">
			<parameter type="int[]"/>
		</method>
		<method name="setSelection">
			<parameter type="org.eclipse.swt.widgets.TableItem[]"/>
		</method>
		<method name="selectAll"/>
		<method name="showSelection"/>
	</methods>
	<!-- PROPERTIES -->
	<properties-preferred names="linesVisible headerVisible"/>
	<properties-advanced names="topIndex"/>
	<!-- PARAMETERS -->
	<parameters>
		<parameter name="layout.has">false</parameter>
		<parameter name="gridLayout.grabHorizontal">true</parameter>
		<parameter name="gridLayout.grabVertical">true</parameter>
	</parameters>
</component>
